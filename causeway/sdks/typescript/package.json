{
  "name": "@mode-7/raceway-node",
  "version": "0.1.0",
  "description": "Official Node.js/TypeScript SDK for Raceway - causal race condition detection for distributed systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.js",
      "types": "./dist/runtime.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "debugging",
    "tracing",
    "distributed-systems",
    "race-conditions",
    "causality",
    "observability"
  ],
  "author": "Raceway",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mode7labs/raceway"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "INSTRUMENTATION-GUIDE.md",
    "CONTRIBUTING.md"
  ]
}
